{
  "manifest_version": 3,
  "name": "findTab",
  "description": "Quickly switch tabs by searching",
  "version": "0.0.8",
  "author": "Erlend Oftedal",

  "icons": {
    "128": "icon.png"
  },

  "permissions": [
    "tabs",
    "storage"
  ],

  "action": {
    "default_icon": "icon.png",
    "default_title": "findTab",
    "default_popup": "popup.html"
  },

  "commands": {
    "open_popup": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "windows": "Ctrl+Shift+M",
        "mac": "Command+Shift+M"
      },
      "description": "Open the findTab popup"
    }
  }
}
